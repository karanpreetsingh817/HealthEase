{
	"name": "zeiw-client",
	"license": "MIT",
	"scripts": {
		"post-build": "rm -rf build/{assets,polyfills*} && cp -r src/assets/* build",
		"build": "preact build --template src/index.html --no-prerender && npm run post-build",
		"start": "preact watch --template src/index.html"
	},
	"dependencies": {
		"@tippyjs/react": "4.2.6",
		"howler": "2.2.3",
		"js-cookie": "3.0.5",
		"jss": "10.10.0",
		"jss-camel-case": "6.1.0",
		"jss-nested": "6.0.1",
		"jss-vendor-prefixer": "8.0.1",
		"micromodal": "0.4.10",
		"preact": "10.13.2",
		"preact-cli": "3.4.5",
		"socket.io-client": "4.6.1",
		"sweetalert2": "11.7.3",
		"tachyons": "4.12.0"
	},
	"devDependencies": {
		"@types/howler": "2.2.7",
		"@types/js-cookie": "3.0.3",
		"@types/micromodal": "0.3.3",
		"@types/node": "18.16.3",
		"@types/prettier": "2.7.2",
		"@types/react": "18.2.0",
		"@types/socket.io-client": "3.0.0",
		"prettier": "2.8.8",
		"typescript": "5.0.4"
	},
	"prettier": {
		"semi": false,
		"useTabs": true,
		"trailingComma": "all"
	}
}
